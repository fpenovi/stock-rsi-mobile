{
  "name": "stock-rsi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "postversion": "react-native-version",
    "make-android-debug": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android/ && ./gradlew assembleDebug"
  },
  "lint-staged": {
    "*.{js, json}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "flux-standard-action": "^2.0.3",
    "react": "16.6.1",
    "react-native": "0.57.5",
    "react-native-config": "^0.11.6",
    "react-native-paper": "^2.2.8",
    "react-native-vector-icons": "^6.1.0",
    "react-navigation": "^2.18.2",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.0"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "husky": "^1.2.0",
    "jest": "23.6.0",
    "lint-staged": "^8.1.0",
    "metro-react-native-babel-preset": "0.49.2",
    "prettier": "^1.15.2",
    "prop-types": "^15.6.2",
    "react-native-version": "^2.6.6",
    "react-test-renderer": "16.6.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
